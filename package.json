{
  "name": "typescript-scaffold",
  "version": "1.0.0",
  "description": "A project scaffold useful for a variety of TypeScript projects",
  "main": "index.ts",
  "scripts": {
    "build": "tsc",
    "check": "./bin/check-vulnerabilities.sh",
    "container-build": "docker build . -t typescript-scaffold-app",
    "container-run": "docker run -p 8080:8080 -d typescript-scaffold-app",
    "container-stop": "docker ps -q --filter ancestor=\"typescript-scaffold-app\" | xargs -r docker stop",
    "lint": "eslint .",
    "start": "nodemon dist/index.js",
    "test": "npm run lint && jest"
  },
  "keywords": [],
  "author": "github.com/sstone-loveall",
  "license": "ISC",
  "devDependencies": {
    "@jest/globals": "^29.6.3",
    "@tsconfig/node20": "^20.1.2",
    "@tsconfig/recommended": "^1.0.2",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.4",
    "@types/node": "^20.5.3",
    "@typescript-eslint/eslint-plugin": "^5.15.0",
    "@typescript-eslint/parser": "^5.15.0",
    "eslint": "^8.11.0",
    "eslint-config-prettier": "^9.0.0",
    "jest": "^29.6.3",
    "jest-extended": "^4.0.1",
    "nodemon": "^3.0.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.7.0",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "source-map-loader": "^4.0.1",
    "ts-loader": "^9.4.4"
  }
}
